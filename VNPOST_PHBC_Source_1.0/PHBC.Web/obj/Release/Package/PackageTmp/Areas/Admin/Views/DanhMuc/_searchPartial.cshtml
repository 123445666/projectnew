@model PHBC.DAO.Models.SysDMTypeSearchModel
@using PHBC.Web.Constants


@using (Html.BeginForm("Search", "DanhMuc", new { Area = "Admin" }, FormMethod.Post, htmlAttributes: new { role = "form", @class = "navbar-form navbar-right" }))
{
    @Html.AntiForgeryToken()
    <div class="input-group">
        @Html.EditorFor(model => model.Name, new { htmlAttributes = new { @class = "form-control", placeholder = "Tên danh mục" } })
        <span class="input-group-btn">
            <button class="btn btn-info" type="submit">
                <i class="@Application.ButtonIcon.Search"></i>
            </button>
        </span>
    </div>
}

